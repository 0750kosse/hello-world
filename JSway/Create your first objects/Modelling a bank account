const account = {
  name:"Alex",
  balance:0,
  credit(n){
    if (this.balance < 0) return "out of money"; },
  describe(){
  return `owner: ${this.name}, balance ${this.balance}`;
  }
};

account.balance+=250;
account.balance-=80;
account.balance-=170;
account.balance-=80;



console.log(account.describe());
