// TODO: create the character object here
const aurora = {
  name : "Aurora",
  health:150,
  strenght: 25,
  xp: 0,
  describe (){
  return `${this.name} has ${this.health}, a strength of ${this.strenght} and her experience is ${this.xp}`
  }
};

aurora.health -= 20;
aurora.strength += 10;
aurora.xp += 15;

console.log(aurora.describe());
